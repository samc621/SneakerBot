      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "wait-on": "^7.0.1"
